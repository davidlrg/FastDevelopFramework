<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ContactsMainActivity"
android:orientation="vertical">

    <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="this is contacts!"
            android:gravity="center_horizontal"
            />
    <Button
            android:id="@+id/bt_contacts_to_main"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="跳转到main"/>
    <Button
            android:id="@+id/bt_contacts_to_mine"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="跳转到mine"/>
    <Button
            android:id="@+id/bt_contacts_to_msg"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="跳转到msg"/>
    <Button
            android:id="@+id/bt_contacts_to_found"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="跳转到found"/>

</LinearLayout>