<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context=".FoundMainActivity">

    <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="this is found!"
            android:gravity="center_horizontal"
    />
    <Button
            android:id="@+id/bt_found_to_main"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="跳转到main"/>
    <Button
            android:id="@+id/bt_found_to_mine"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="跳转到mine"/>
    <Button
            android:id="@+id/bt_found_to_msg"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="跳转到msg"/>
    <Button
            android:id="@+id/bt_found_to_contacts"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="跳转到contacts"/>
</LinearLayout>